        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.Data.StateChangeEventArgs.CurrentState">
          <source>Gets the new state of the connection.</source>
          <target state="translated">取得連線的新狀態。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="P:System.Data.StateChangeEventArgs.CurrentState">
