      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Runtime.InteropServices.ComInterfaceType">
          <source>Identifies how to expose an interface to COM.</source>
          <target state="translated">識別如何將介面公開至 COM。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Runtime.InteropServices.ComInterfaceType">
          <source>This enumeration works in conjunction with the <ph id="ph1">&lt;xref:System.Runtime.InteropServices.InterfaceTypeAttribute&gt;</ph> attribute.</source>
          <target state="translated">這個列舉可搭配<ph id="ph1">&lt;xref:System.Runtime.InteropServices.InterfaceTypeAttribute&gt;</ph>屬性。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Runtime.InteropServices.ComInterfaceType.InterfaceIsDual">
          <source>Indicates that the interface is exposed to COM as a <bpt id="p1">[</bpt>dual interface<ept id="p1">](http://msdn.microsoft.com/library/1d39d64c-e23a-4249-a167-4369d5bd2b65)</ept>, which enables both early and late binding.</source>
          <target state="translated">指出介面向 COM 公開為<bpt id="p1">[</bpt>雙重介面<ept id="p1">](http://msdn.microsoft.com/library/1d39d64c-e23a-4249-a167-4369d5bd2b65)</ept>，可使用早期繫結和晚期繫結。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Runtime.InteropServices.ComInterfaceType.InterfaceIsDual">
          <source><ph id="ph1">&lt;see cref="F:System.Runtime.InteropServices.ComInterfaceType.InterfaceIsDual" /&gt;</ph> is the default value.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="F:System.Runtime.InteropServices.ComInterfaceType.InterfaceIsDual" /&gt;</ph> 為預設值。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Runtime.InteropServices.ComInterfaceType.InterfaceIsIDispatch">
          <source>Indicates that an interface is exposed to COM as a <bpt id="p1">[</bpt>dispinterface<ept id="p1">](http://msdn.microsoft.com/library/e8c18ae5-3d9e-4dff-aa20-b5acc723eacf)</ept>, which enables late binding only.</source>
          <target state="translated">指出介面向 COM 公開為 <bpt id="p1">[</bpt>dispinterface<ept id="p1">](http://msdn.microsoft.com/library/e8c18ae5-3d9e-4dff-aa20-b5acc723eacf)</ept>，僅可使用晚期繫結。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Runtime.InteropServices.ComInterfaceType.InterfaceIsIInspectable">
          <source>Indicates that an interface is exposed to COM as a <ph id="ph1">[!INCLUDE[wrt](~/includes/wrt-md.md)]</ph> interface.</source>
          <target state="translated">指出介面向 COM 公開為 <ph id="ph1">[!INCLUDE[wrt](~/includes/wrt-md.md)]</ph> 介面。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Runtime.InteropServices.ComInterfaceType.InterfaceIsIUnknown">
          <source>Indicates that an interface is exposed to COM as an interface that is derived from <bpt id="p1">[</bpt>IUnknown<ept id="p1">](http://go.microsoft.com/fwlink/?LinkId=148003)</ept>, which enables only early binding.</source>
          <target state="translated">指出介面向 COM 公開為自 <bpt id="p1">[</bpt>IUnknown<ept id="p1">](http://go.microsoft.com/fwlink/?LinkId=148003)</ept> 衍生的介面，僅可使用早期繫結。</target>       </trans-unit>
