        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Runtime.InteropServices.UCOMIConnectionPointContainer.EnumConnectionPoints(System.Runtime.InteropServices.UCOMIEnumConnectionPoints@)">
