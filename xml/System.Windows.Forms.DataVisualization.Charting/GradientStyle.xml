      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.DataVisualization.Charting.GradientStyle">
          <source>Specifies a gradient style to be used for a chart element.</source>
          <target state="translated">指定要用於圖表項目的漸層樣式。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.DataVisualization.Charting.GradientStyle">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.DataVisualization.Charting.GradientStyle&gt;</ph> enumeration represents a gradient style, and is used with the <ph id="ph2">&lt;xref:System.Windows.Forms.DataVisualization.Charting.Chart.BackGradientStyle%2A&gt;</ph> property of the <ph id="ph3">&lt;xref:System.Windows.Forms.DataVisualization.Charting.Chart&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Windows.Forms.DataVisualization.Charting.GradientStyle&gt;</ph>列舉型別所表示的漸層的樣式，並配合<ph id="ph2">&lt;xref:System.Windows.Forms.DataVisualization.Charting.Chart.BackGradientStyle%2A&gt;</ph>屬性<ph id="ph3">&lt;xref:System.Windows.Forms.DataVisualization.Charting.Chart&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.GradientStyle.Center">
          <source>Gradient is applied from the center outwards.</source>
          <target state="translated">從中央向外套用漸層。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.GradientStyle.DiagonalLeft">
          <source>Gradient is applied diagonally from left to right.</source>
          <target state="translated">以對角方式從左到右套用漸層。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.GradientStyle.DiagonalRight">
          <source>Gradient is applied diagonally from right to left.</source>
          <target state="translated">以對角方式從右到左套用漸層。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.GradientStyle.HorizontalCenter">
          <source>Gradient is applied horizontally from the center outwards.</source>
          <target state="translated">以水平方式從中央向外套用漸層。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.GradientStyle.LeftRight">
          <source>Gradient is applied from left to right.</source>
          <target state="translated">從左到右套用漸層。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.GradientStyle.None">
          <source>No gradient is applied.</source>
          <target state="translated">不套用任何漸層。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.GradientStyle.TopBottom">
          <source>Gradient is applied from top to bottom.</source>
          <target state="translated">從上到下套用漸層。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.GradientStyle.VerticalCenter">
          <source>Gradient is applied vertically from the center outwards.</source>
          <target state="translated">以垂直方式從中央向外套用漸層。</target>       </trans-unit>
