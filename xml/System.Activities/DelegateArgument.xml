      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.DelegateArgument">
          <source>The abstract base class that represents a binding terminal that supports the flow of data into and out of an <ph id="ph1">&lt;see cref="T:System.Activities.ActivityDelegate" /&gt;</ph>.</source>
          <target state="translated">表示支援將資料傳入 <ph id="ph1">&lt;see cref="T:System.Activities.ActivityDelegate" /&gt;</ph> 和從當中傳出資料之繫結終端機的抽象基底類別。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="P:System.Activities.DelegateArgument.Direction">
          <source>Gets an <ph id="ph1">&lt;see cref="T:System.Activities.ArgumentDirection" /&gt;</ph> that specifies whether the <ph id="ph2">&lt;see cref="T:System.Activities.DelegateArgument" /&gt;</ph> represents the flow of data into an <ph id="ph3">&lt;see cref="T:System.Activities.ActivityDelegate" /&gt;</ph>, out of an <ph id="ph4">&lt;see cref="T:System.Activities.ActivityDelegate" /&gt;</ph>, or both into and out of an <ph id="ph5">&lt;see cref="T:System.Activities.ActivityDelegate" /&gt;</ph>.</source>
          <target state="translated">取得 <ph id="ph1">&lt;see cref="T:System.Activities.ArgumentDirection" /&gt;</ph>，指定 <ph id="ph2">&lt;see cref="T:System.Activities.DelegateArgument" /&gt;</ph> 是否表示資料傳入 <ph id="ph3">&lt;see cref="T:System.Activities.ActivityDelegate" /&gt;</ph>、從 <ph id="ph4">&lt;see cref="T:System.Activities.ActivityDelegate" /&gt;</ph><ph id="ph5">&lt;see cref="T:System.Activities.ActivityDelegate" /&gt;</ph>傳出，或由  傳入與傳出。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.DelegateArgument.Direction">
          <source>One of the enumeration values that specifies the direction of the flow of data through the <ph id="ph1">&lt;see cref="T:System.Activities.DelegateArgument" /&gt;</ph>.</source>
          <target state="translated">其中一個列舉值，指定流經 <ph id="ph1">&lt;see cref="T:System.Activities.DelegateArgument" /&gt;</ph> 之資料的方向。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Activities.DelegateArgument.Get(System.Activities.ActivityContext)">
          <source>The current activity context.</source>
          <target state="translated">目前的活動內容。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Activities.DelegateArgument.Get(System.Activities.ActivityContext)">
          <source>Returns the value of the argument using the specified activity context.</source>
          <target state="translated">使用指定的活動內容傳回引數的值。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Activities.DelegateArgument.Get(System.Activities.ActivityContext)">
          <source>The value of the argument.</source>
          <target state="translated">引數的值。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Activities.DelegateArgument.GetLocation(System.Activities.ActivityContext)">
          <source>The current activity context.</source>
          <target state="translated">目前的活動內容。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Activities.DelegateArgument.GetLocation(System.Activities.ActivityContext)">
          <source>Returns the <ph id="ph1">&lt;see cref="T:System.Activities.Location" /&gt;</ph> of the value referenced by the <ph id="ph2">&lt;see cref="T:System.Activities.DelegateArgument" /&gt;</ph>.</source>
          <target state="translated">傳回 <ph id="ph1">&lt;see cref="T:System.Activities.Location" /&gt;</ph> 所參考的 <ph id="ph2">&lt;see cref="T:System.Activities.DelegateArgument" /&gt;</ph> 值。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Activities.DelegateArgument.GetLocation(System.Activities.ActivityContext)">
          <source>The location of the value referenced by the <ph id="ph1">&lt;see cref="T:System.Activities.DelegateArgument" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Activities.DelegateArgument" /&gt;</ph> 參考之值的位置。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:System.Activities.DelegateArgument.Name">
          <source>Gets or sets the name of the <ph id="ph1">&lt;see cref="T:System.Activities.DelegateArgument" /&gt;</ph>.</source>
          <target state="translated">取得或設定 <ph id="ph1">&lt;see cref="T:System.Activities.DelegateArgument" /&gt;</ph> 的名稱。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.DelegateArgument.Name">
          <source>The name of the <ph id="ph1">&lt;see cref="T:System.Activities.DelegateArgument" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Activities.DelegateArgument" /&gt;</ph> 的名稱。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="P:System.Activities.DelegateArgument.NameCore">
          <source>Gets the string value used by <ph id="ph1">&lt;see cref="P:System.Activities.DelegateArgument.Name" /&gt;</ph>.</source>
