        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.SqlClient.PoolBlockingPeriod">
          <source>Specifies a value for the <ph id="ph1">&lt;see cref="P:System.Data.SqlClient.SqlConnectionStringBuilder.PoolBlockingPeriod" /&gt;</ph> property.</source>
          <target state="translated">指定 <ph id="ph1">&lt;see cref="P:System.Data.SqlClient.SqlConnectionStringBuilder.PoolBlockingPeriod" /&gt;</ph> 屬性的值。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Data.SqlClient.PoolBlockingPeriod.AlwaysBlock">
          <source>Blocking period ON for all SQL servers including Azure SQL servers.</source>
          <target state="translated">所有 SQL Server (包括 Azure SQL Server) 的封鎖期間均為 ON。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Data.SqlClient.PoolBlockingPeriod.Auto">
          <source>Blocking period OFF for Azure SQL servers, but ON for all other SQL servers.</source>
          <target state="translated">Azure SQL Server 的封鎖期間為 OFF，但所有其他 SQL Server 的封鎖期間為 ON。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Data.SqlClient.PoolBlockingPeriod.NeverBlock">
          <source>Blocking period OFF for all SQL servers including Azure SQL servers.</source>
          <target state="translated">所有 SQL Server (包括 Azure SQL Server) 的封鎖期間均為 OFF。</target>       </trans-unit>
