      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Data.SqlClient.SqlEnclaveSession">
          <source>Encapsulates the state of a secure session between SqlClient and an enclave inside SQL Server, which can be used for computations on encrypted columns protected with Always Encrypted.</source>
          <target state="translated">封裝 SqlClient 與 SQL Server 內之 enclave 間的安全工作階段狀態，enclave 可用於計算以 Always Encrypted 保護的加密資料行。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Data.SqlClient.SqlEnclaveSession.#ctor(System.Byte[],System.Int64)">
          <source>The symmetric key used to encrypt all the information sent using the session.</source>
          <target state="translated">用來為使用工作階段傳送之所有資訊加密的對稱金鑰。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Data.SqlClient.SqlEnclaveSession.#ctor(System.Byte[],System.Int64)">
          <source>The session ID.</source>
          <target state="translated">工作階段識別碼。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Data.SqlClient.SqlEnclaveSession.#ctor(System.Byte[],System.Int64)">
          <source>Instantiates a new instance of the <ph id="ph1">&lt;see cref="T:System.Data.SqlClient.SqlEnclaveSession" /&gt;</ph> class.</source>
          <target state="translated">執行個體化 <ph id="ph1">&lt;see cref="T:System.Data.SqlClient.SqlEnclaveSession" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Data.SqlClient.SqlEnclaveSession.#ctor(System.Byte[],System.Int64)">
          <source><ph id="ph1">&lt;paramref name="sessionKey" /&gt;</ph> is <ph id="ph2">&lt;see langword="null" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;paramref name="sessionKey" /&gt;</ph> 為 <ph id="ph2">&lt;see langword="null" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Data.SqlClient.SqlEnclaveSession.#ctor(System.Byte[],System.Int64)">
          <source><ph id="ph1">&lt;paramref name="sessionKey" /&gt;</ph> has zero length.</source>
          <target state="translated"><ph id="ph1">&lt;paramref name="sessionKey" /&gt;</ph> 長度為零。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Data.SqlClient.SqlEnclaveSession.GetSessionKey">
          <source>Gets the symmetric key that SqlClient uses to encrypt all the information it sends to the enclave using the session.</source>
          <target state="translated">取得 SqlClient 用來為使用工作階段傳送到飛地之所有資訊加密的對稱金鑰。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Data.SqlClient.SqlEnclaveSession.GetSessionKey">
          <source>The symmetric key.</source>
          <target state="translated">對稱金鑰。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="P:System.Data.SqlClient.SqlEnclaveSession.SessionId">
          <source>Gets the session ID.</source>
          <target state="translated">取得工作階段識別碼。</target>       </trans-unit>
