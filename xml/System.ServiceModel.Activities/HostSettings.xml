      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Activities.HostSettings">
          <source>Represents settings that define operational characteristics of a host.</source>
          <target state="translated">代表可定義主機之作業特性的設定。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Activities.HostSettings.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.Activities.HostSettings" /&gt;</ph> class.</source>
          <target state="translated">初始化 <ph id="ph1">&lt;see cref="T:System.ServiceModel.Activities.HostSettings" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Activities.HostSettings.IncludeExceptionDetailInFaults">
          <source>Gets or sets whether exception details are included in faults.</source>
          <target state="translated">取得或設定錯誤中是否包含例外狀況詳細資料。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Activities.HostSettings.IncludeExceptionDetailInFaults">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if exception details are included in faults; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果在錯誤中包含例外狀況的詳細資訊，則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為 <ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Activities.HostSettings.ScopeName">
          <source>Gets or sets the name of scope.</source>
          <target state="translated">取得或設定範圍的名稱。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Activities.HostSettings.ScopeName">
          <source>The name of scope.</source>
          <target state="translated">範圍的名稱。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Activities.HostSettings.UseNoPersistHandle">
          <source>Gets or sets a value that indicates whether to use non-persistent handle.</source>
          <target state="translated">取得或設定值，指出是否要使用非持續性的控制代碼。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Activities.HostSettings.UseNoPersistHandle">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if non-persistent handle is used; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果使用非持續性的控制代碼，則為 <ph id="ph1">&lt;see langword="true" /&gt;</ph>，否則為<ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
