      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.WebControls.VerticalAlign">
          <source>Specifies the vertical alignment of an object or text in a control.</source>
          <target state="translated">指定控制項中物件或文字的垂直對齊。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.UI.WebControls.VerticalAlign">
          <source>The <ph id="ph1">&lt;xref:System.Web.UI.WebControls.VerticalAlign&gt;</ph> enumeration represents the different vertical alignment options for an object or text in a control.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Web.UI.WebControls.VerticalAlign&gt;</ph>列舉，代表物件或文字在控制項中的各種不同的垂直對齊選項。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Web.UI.WebControls.VerticalAlign.Bottom">
          <source>Text or object is aligned with the bottom of the enclosing control.</source>
          <target state="translated">文字或物件對齊封入控制項的下方。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Web.UI.WebControls.VerticalAlign.Middle">
          <source>Text or object is aligned with the center of the enclosing control.</source>
          <target state="translated">文字或物件對齊封入控制項的中央。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Web.UI.WebControls.VerticalAlign.NotSet">
          <source>Vertical alignment is not set.</source>
          <target state="translated">未設定垂直對齊。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Web.UI.WebControls.VerticalAlign.Top">
          <source>Text or object is aligned with the top of the enclosing control.</source>
          <target state="translated">文字或物件對齊封入控制項的上方。</target>       </trans-unit>
