        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.DataVisualization.Charting.ChartImageAlignmentStyle">
          <source>Specifies the image alignment for a chart background image.</source>
          <target state="translated">指定圖表背景影像的影像對齊方式。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.ChartImageAlignmentStyle.Bottom">
          <source>The image is aligned to the bottom boundary of the chart element.</source>
          <target state="translated">影像會對齊圖表項目的下界限。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.ChartImageAlignmentStyle.BottomLeft">
