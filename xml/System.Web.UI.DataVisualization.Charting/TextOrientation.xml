      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.DataVisualization.Charting.TextOrientation">
          <source>Specifies text orientation in a chart element.</source>
          <target state="translated">指定圖表項目的文字方向。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.TextOrientation.Auto">
          <source>Text orientation is automatically determined, based on the type of chart element in which the text appears.</source>
          <target state="translated">會根據文字所在圖表項目的類型，自動決定文字方向。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.TextOrientation.Horizontal">
