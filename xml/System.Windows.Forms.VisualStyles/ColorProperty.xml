      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.VisualStyles.ColorProperty">
          <source>Identifies the color properties of a visual style element.</source>
          <target state="translated">識別視覺化樣式項目的色彩屬性。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.VisualStyles.ColorProperty">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.ColorProperty&gt;</ph> enumeration represents a set of color properties that describe visual style elements.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.ColorProperty&gt;</ph>列舉，代表一組描述視覺化樣式元素的色彩屬性。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.VisualStyles.ColorProperty">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.ColorProperty&gt;</ph> values are used as an argument in the <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleRenderer.GetColor%2A?displayProperty=nameWithType&gt;</ph> method to specify which property value to retrieve for the element that the <ph id="ph3">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleRenderer&gt;</ph> currently represents.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.ColorProperty&gt;</ph>值當做引數用於<ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleRenderer.GetColor%2A?displayProperty=nameWithType&gt;</ph>方法，以指定要擷取項目的的屬性值，<ph id="ph3">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleRenderer&gt;</ph>目前表示。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.AccentColorHint">
          <source>A recommended companion color for the accent color of the visual style.</source>
          <target state="translated">針對視覺化樣式的強調色彩所建議的搭配色彩。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.BorderColor">
          <source>The border color of an element with a filled-border background.</source>
          <target state="translated">具有填滿框線背景的項目之框線色彩。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.BorderColorHint">
          <source>A recommended companion color for the border color of the visual style.</source>
          <target state="translated">針對視覺化樣式的框線色彩所建議的搭配色彩。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.EdgeDarkShadowColor">
          <source>The dark shadow color for edges.</source>
          <target state="translated">邊緣的深色陰影色彩。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.EdgeFillColor">
          <source>The fill color for edges.</source>
          <target state="translated">邊緣的填滿色彩。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.EdgeHighlightColor">
          <source>The highlight color for edges.</source>
          <target state="translated">邊緣的反白色彩。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.EdgeLightColor">
          <source>The light color for edges.</source>
          <target state="translated">邊緣的淺色色彩。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.EdgeShadowColor">
          <source>The shadow color for edges.</source>
          <target state="translated">邊緣的陰影色彩。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.FillColor">
          <source>The fill color of an element with a filled-border background.</source>
          <target state="translated">具有填滿框線背景的項目之填滿色彩。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.FillColorHint">
          <source>A recommended companion color for the fill color of the visual style.</source>
          <target state="translated">針對視覺化樣式的填滿色彩所建議的搭配色彩。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.GlowColor">
          <source>The glow color.</source>
          <target state="translated">光暈的色彩。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.GlyphTextColor">
          <source>The color that a font-based glyph is drawn with.</source>
          <target state="translated">用來描繪字型架構圖像 (Glyph) 的色彩。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.GlyphTransparentColor">
          <source>The color of pixels in a glyph that are treated as transparent.</source>
          <target state="translated">圖像中視為透明的像素色彩。</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.GradientColor1">
          <source>The first color in a gradient.</source>
          <target state="translated">漸層的第一個色彩。</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.GradientColor2">
          <source>The second color in a gradient.</source>
          <target state="translated">漸層的第二個色彩。</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.GradientColor3">
          <source>The third color in a gradient.</source>
          <target state="translated">漸層的第三個色彩。</target>       </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.GradientColor4">
          <source>The fourth color in a gradient.</source>
          <target state="translated">漸層的第四個色彩。</target>       </trans-unit>
        <trans-unit id="121" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.GradientColor5">
          <source>The fifth color in a gradient.</source>
          <target state="translated">漸層的第五個色彩。</target>       </trans-unit>
        <trans-unit id="122" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.ShadowColor">
          <source>The color of the shadow.</source>
          <target state="translated">陰影的色彩。</target>       </trans-unit>
        <trans-unit id="123" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.TextBorderColor">
          <source>The color of the text border.</source>
          <target state="translated">文字框線的色彩。</target>       </trans-unit>
        <trans-unit id="124" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.TextColor">
          <source>The text color.</source>
          <target state="translated">文字的色彩。</target>       </trans-unit>
        <trans-unit id="125" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.TextShadowColor">
          <source>The color of the text shadow.</source>
          <target state="translated">文字陰影的色彩。</target>       </trans-unit>
        <trans-unit id="126" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ColorProperty.TransparentColor">
          <source>The color of pixels that are treated as transparent.</source>
          <target state="translated">視為透明的像素色彩。</target>       </trans-unit>
