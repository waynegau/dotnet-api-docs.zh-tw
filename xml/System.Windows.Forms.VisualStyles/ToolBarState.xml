      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.VisualStyles.ToolBarState">
          <source>Specifies the visual state of a toolbar item that is drawn with visual styles.</source>
          <target state="translated">指定利用視覺化樣式繪製的工具列項目之可見狀態。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.VisualStyles.ToolBarState">
          <source>The <ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.ToolBarState&gt;</ph> values correspond to the states of the <ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement&gt;</ph> objects exposed by the <ph id="ph3">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement.ToolBar.Button&gt;</ph>, <ph id="ph4">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement.ToolBar.DropDownButton&gt;</ph>, <ph id="ph5">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement.ToolBar.SplitButton&gt;</ph>, and <ph id="ph6">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement.ToolBar.SplitButtonDropDown&gt;</ph> classes.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Windows.Forms.VisualStyles.ToolBarState&gt;</ph>值會對應至狀態<ph id="ph2">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement&gt;</ph>物件所公開<ph id="ph3">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement.ToolBar.Button&gt;</ph>， <ph id="ph4">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement.ToolBar.DropDownButton&gt;</ph>， <ph id="ph5">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement.ToolBar.SplitButton&gt;</ph>，和<ph id="ph6">&lt;xref:System.Windows.Forms.VisualStyles.VisualStyleElement.ToolBar.SplitButtonDropDown&gt;</ph>類別。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ToolBarState.Checked">
          <source>The item is in the checked state.</source>
          <target state="translated">項目處於選取狀態。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ToolBarState.Disabled">
          <source>The item is in the disabled state.</source>
          <target state="translated">項目處於停用狀態。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ToolBarState.Hot">
          <source>The item is in the hot state.</source>
          <target state="translated">項目處於作用中狀態。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ToolBarState.HotChecked">
          <source>The item is in the hot and checked states.</source>
          <target state="translated">項目處於作用中且選取狀態。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ToolBarState.Normal">
          <source>The item is in the normal state.</source>
          <target state="translated">項目處於正常狀態。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.VisualStyles.ToolBarState.Pressed">
          <source>The item is in the pressed state.</source>
          <target state="translated">項目處於已按下狀態。</target>       </trans-unit>
