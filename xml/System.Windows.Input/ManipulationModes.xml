      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Input.ManipulationModes">
          <source>Specifies how manipulation events are interpreted.</source>
          <target state="translated">指定如何解譯操作事件。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ManipulationModes.All">
          <source>A manipulation can scale, translate, or rotate an object and can occur with one point of input.</source>
          <target state="translated">操作可以縮放、平移或旋轉物件，而且可以利用一個輸入點執行。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ManipulationModes.None">
          <source>Manipulation events do not occur.</source>
          <target state="translated">不發生操作事件。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ManipulationModes.Rotate">
          <source>A manipulation can rotate an object.</source>
          <target state="translated">操作可以旋轉物件。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ManipulationModes.Scale">
          <source>A manipulation can scale an object.</source>
          <target state="translated">操作可以縮放物件。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ManipulationModes.Translate">
          <source>A manipulation can translate an object.</source>
          <target state="translated">操作可以平移物件。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Input.ManipulationModes.TranslateX">
          <source>A manipulation can translate an object horizontally.</source>
          <target state="translated">操作可以水平平移物件。</target>       </trans-unit>
