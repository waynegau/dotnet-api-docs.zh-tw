        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.CodeDom.CodeEventReferenceExpression">
          <source>Represents a reference to an event.</source>
          <target state="translated">代表事件的參考。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.CodeDom.CodeEventReferenceExpression">
          <source><ph id="ph1">&lt;xref:System.CodeDom.CodeEventReferenceExpression&gt;</ph> can be used to represent a reference to an event.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.CodeDom.CodeEventReferenceExpression&gt;</ph> 可用來代表事件的參考。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.CodeDom.CodeEventReferenceExpression">
          <source>The <ph id="ph1">&lt;xref:System.CodeDom.CodeEventReferenceExpression.TargetObject%2A&gt;</ph> property specifies the object that contains the event.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.CodeDom.CodeEventReferenceExpression.TargetObject%2A&gt;</ph>屬性會指定包含此事件的物件。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.CodeDom.CodeEventReferenceExpression">
          <source>The <ph id="ph1">&lt;xref:System.CodeDom.CodeEventReferenceExpression.EventName%2A&gt;</ph> property specifies the name of the event.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.CodeDom.CodeEventReferenceExpression.EventName%2A&gt;</ph>屬性會指定事件的名稱。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.CodeDom.CodeEventReferenceExpression">
