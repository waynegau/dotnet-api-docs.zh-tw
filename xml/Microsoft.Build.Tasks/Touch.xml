          <target state="translated">執行 <bpt id="p1">&lt;c&gt;</bpt>Touch<ept id="p1">&lt;/c&gt;</ept> 工作。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.Touch.Execute">
