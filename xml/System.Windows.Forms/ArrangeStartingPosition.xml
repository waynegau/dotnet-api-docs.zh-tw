      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.ArrangeStartingPosition">
          <source>Specifies the starting position that the system uses to arrange minimized windows.</source>
          <target state="translated">指定系統用來排列最小化視窗的開始位置。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Forms.ArrangeStartingPosition">
          <source>This enumeration is used by the <ph id="ph1">&lt;xref:System.Windows.Forms.SystemInformation.ArrangeStartingPosition%2A&gt;</ph> property of the <ph id="ph2">&lt;xref:System.Windows.Forms.SystemInformation&gt;</ph> class.</source>
          <target state="translated">這個列舉型別由<ph id="ph1">&lt;xref:System.Windows.Forms.SystemInformation.ArrangeStartingPosition%2A&gt;</ph>屬性<ph id="ph2">&lt;xref:System.Windows.Forms.SystemInformation&gt;</ph>類別。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ArrangeStartingPosition.BottomLeft">
          <source>Starts at the lower-left corner of the screen, which is the default position.</source>
          <target state="translated">從畫面的左下角開始，此為預設位置。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ArrangeStartingPosition.BottomRight">
          <source>Starts at the lower-right corner of the screen.</source>
          <target state="translated">從畫面的右下角開始。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ArrangeStartingPosition.Hide">
          <source>Hides minimized windows by moving them off the visible area of the screen.</source>
          <target state="translated">將最小化視窗移出畫面的可見區以隱藏起來。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ArrangeStartingPosition.TopLeft">
          <source>Starts at the upper-left corner of the screen.</source>
          <target state="translated">從畫面的左上角開始。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ArrangeStartingPosition.TopRight">
          <source>Starts at the upper-right corner of the screen.</source>
          <target state="translated">從畫面的右上角開始。</target>       </trans-unit>
