    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.TextImageRelation">
          <source>Specifies the position of the text and image relative to each other on a control.</source>
          <target state="translated">指定在控制項上文字和影像的相對位置。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.TextImageRelation.ImageAboveText">
          <source>Specifies that the image is displayed vertically above the text of a control.</source>
          <target state="translated">指定影像垂直顯示在控制項文字之上。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.TextImageRelation.ImageBeforeText">
          <source>Specifies that the image is displayed horizontally before the text of a control.</source>
          <target state="translated">指定影像水平顯示在控制項文字之前。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.TextImageRelation.Overlay">
          <source>Specifies that the image and text share the same space on a control.</source>
          <target state="translated">指定影像和文字共用控制項上相同的空間。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.TextImageRelation.TextAboveImage">
          <source>Specifies that the text is displayed vertically above the image of a control.</source>
          <target state="translated">指定文字垂直顯示在控制項影像之上。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.TextImageRelation.TextBeforeImage">
          <source>Specifies that the text is displayed horizontally before the image of a control.</source>
          <target state="translated">指定文字水平顯示在控制項影像之前。</target>       </trans-unit>
