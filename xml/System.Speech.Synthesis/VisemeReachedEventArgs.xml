      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Speech.Synthesis.VisemeReachedEventArgs">
          <source>Returns data from the <ph id="ph1">&lt;see cref="E:System.Speech.Synthesis.SpeechSynthesizer.VisemeReached" /&gt;</ph> event.</source>
          <target state="translated">從 <ph id="ph1">&lt;see cref="E:System.Speech.Synthesis.SpeechSynthesizer.VisemeReached" /&gt;</ph> 事件傳回資料。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Speech.Synthesis.VisemeReachedEventArgs">
          <source>A viseme is the basic position of the mouth and face when pronouncing a phoneme.</source>
          <target state="translated">Viseme 時說音素基本說話和圖示的位置。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Speech.Synthesis.VisemeReachedEventArgs">
          <source>Visemes are visual representations of phonemes.</source>
          <target state="translated">Visemes 是音素的視覺化呈現。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Speech.Synthesis.VisemeReachedEventArgs">
          <source>For more information about visemes, see <ph id="ph1">&lt;xref:System.Speech.Synthesis.SpeechSynthesizer.VisemeReached&gt;</ph>.</source>
          <target state="translated">如需 visemes 的詳細資訊，請參閱<ph id="ph1">&lt;xref:System.Speech.Synthesis.SpeechSynthesizer.VisemeReached&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.Speech.Synthesis.VisemeReachedEventArgs.AudioPosition">
          <source>Gets the position of the viseme in the audio stream.</source>
          <target state="translated">取得音訊資料流中視素 (Viseme) 的位置</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Speech.Synthesis.VisemeReachedEventArgs.AudioPosition">
          <source>A <ph id="ph1">&lt;see cref="T:System.TimeSpan" /&gt;</ph> object that represents the position of the viseme.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.TimeSpan" /&gt;</ph> 物件，表示視素 (Viseme) 的位置。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.Speech.Synthesis.VisemeReachedEventArgs.Duration">
          <source>Gets the duration of the viseme.</source>
          <target state="translated">取得視素 (Viseme) 的持續期間。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Speech.Synthesis.VisemeReachedEventArgs.Duration">
          <source>A <ph id="ph1">&lt;see cref="T:System.TimeSpan" /&gt;</ph> object that represents the duration of the viseme.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.TimeSpan" /&gt;</ph> 物件，表示視素 (Viseme) 的持續期間。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="P:System.Speech.Synthesis.VisemeReachedEventArgs.Emphasis">
          <source>Gets a <ph id="ph1">&lt;see cref="T:System.Speech.Synthesis.SynthesizerEmphasis" /&gt;</ph> object that describes the emphasis of the viseme.</source>
          <target state="translated">取得描述視素 (Viseme) 強調的 <ph id="ph1">&lt;see cref="T:System.Speech.Synthesis.SynthesizerEmphasis" /&gt;</ph> 物件。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Speech.Synthesis.VisemeReachedEventArgs.Emphasis">
          <source>A <ph id="ph1">&lt;see cref="T:System.Speech.Synthesis.SynthesizerEmphasis" /&gt;</ph> object that represents the emphasis of the viseme.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:System.Speech.Synthesis.SynthesizerEmphasis" /&gt;</ph> 物件，表示視素 (Viseme) 的強調。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="P:System.Speech.Synthesis.VisemeReachedEventArgs.NextViseme">
          <source>Gets the value of the next viseme.</source>
          <target state="translated">取得下一個視素 (Viseme) 的值。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Speech.Synthesis.VisemeReachedEventArgs.NextViseme">
          <source>An <ph id="ph1">&lt;see cref="T:System.Int32" /&gt;</ph> object that contains the value of the next viseme.</source>
          <target state="translated">包含下一個視素 (Viseme) 值的 <ph id="ph1">&lt;see cref="T:System.Int32" /&gt;</ph> 物件。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="P:System.Speech.Synthesis.VisemeReachedEventArgs.Viseme">
          <source>Gets the value of the viseme.</source>
          <target state="translated">取得視素 (Viseme) 的值。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Speech.Synthesis.VisemeReachedEventArgs.Viseme">
          <source>An <ph id="ph1">&lt;see cref="T:System.Int32" /&gt;</ph> object that contains the value of the viseme.</source>
          <target state="translated">物件視素 (Viseme) 值的 <ph id="ph1">&lt;see cref="T:System.Int32" /&gt;</ph> 物件。</target>       </trans-unit>
