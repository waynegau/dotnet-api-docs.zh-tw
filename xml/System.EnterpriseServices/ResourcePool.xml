        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.EnterpriseServices.ResourcePool">
          <source>Stores objects in the current transaction.</source>
          <target state="translated">在目前交易中儲存物件。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.EnterpriseServices.ResourcePool">
          <source>This class cannot be inherited.</source>
          <target state="translated">這個類別無法被繼承。</target>       </trans-unit>
