        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Web.Services.Description.HttpBinding.Namespace">
          <source>Specifies the URI for the XML namespace representing the HTTP transport for use with SOAP.</source>
          <target state="translated">指定 XML 命名空間的 URI，表示用於 SOAP 的 HTTP 傳輸。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Web.Services.Description.HttpBinding.Namespace">
          <source>This field is constant.</source>
          <target state="translated">這個欄位是常數。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="F:System.Web.Services.Description.HttpBinding.Namespace">
          <source>The field value is "<ph id="ph1">http://schemas.xmlsoap.org/wsdl/http/</ph>".</source>
          <target state="translated">欄位值是"<ph id="ph1">http://schemas.xmlsoap.org/wsdl/http/</ph>"。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" extradata="MT" uid="F:System.Web.Services.Description.HttpBinding.Namespace">
