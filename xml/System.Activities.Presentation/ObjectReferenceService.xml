      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.Presentation.ObjectReferenceService">
          <source>This interface is used by Visual Studio integration to acquire an AppDomain serialization friendly reference to an object.</source>
          <target state="translated">此介面被 Visual Studio 整合用來取得物件的 AppDomain 序列化易記參考。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.#ctor(System.Activities.Presentation.EditingContext)">
          <source>The EditingContext of the current WorkflowDesigner.</source>
          <target state="translated">目前的 WorkflowDesigner 的 EditingContext。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.#ctor(System.Activities.Presentation.EditingContext)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Activities.Presentation.ObjectReferenceService" /&gt;</ph> class with a specified editing context</source>
          <target state="translated">使用指定的編輯內容，初始化 <ph id="ph1">&lt;see cref="T:System.Activities.Presentation.ObjectReferenceService" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="T:System.Activities.Presentation.ObjectReferenceService">
          <source>Acquires an actual reference of an object.</source>
          <target state="translated">取得物件的實際參考。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.AcquireObjectReference(System.Object)">
          <source>The object which we need to acquire a reference for.</source>
          <target state="translated">我們必須取得其參考的物件。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.AcquireObjectReference(System.Object)">
          <source>Acquires a reference of an object by its actual reference.</source>
          <target state="translated">根據物件的實際參考取得其參考。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.AcquireObjectReference(System.Object)">
          <source>A reference of an object.</source>
          <target state="translated">物件的參考。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.AcquireObjectReference(System.Int32,System.Int32,System.Int32,System.Int32)">
          <source>The start line of the object.</source>
          <target state="translated">物件的起始行。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.AcquireObjectReference(System.Int32,System.Int32,System.Int32,System.Int32)">
          <source>The start column of the object.</source>
          <target state="translated">物件的起始欄。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.AcquireObjectReference(System.Int32,System.Int32,System.Int32,System.Int32)">
          <source>The end line of the object.</source>
          <target state="translated">物件的結尾行。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.AcquireObjectReference(System.Int32,System.Int32,System.Int32,System.Int32)">
          <source>The end column of the object.</source>
          <target state="translated">物件的結尾資料行。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.AcquireObjectReference(System.Int32,System.Int32,System.Int32,System.Int32)">
          <source>Acquires a reference by the SourceLocation of the object and this method will automatically register the object to listen to SourceLocationUpdatedEvent, if available.</source>
          <target state="translated">根據物件的 SourceLocation 取得參考，如果有的話，這個方法會自動註冊該物件以接聽 SourceLocationUpdatedEvent。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.AcquireObjectReference(System.Int32,System.Int32,System.Int32,System.Int32)">
          <source>A reference by the SourceLocation of the object</source>
          <target state="translated">以物件之 SourceLocation 為依據的參考</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.ReleaseObjectReference(System.Guid)">
          <source>The activity reference.</source>
          <target state="translated">活動參考。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.ReleaseObjectReference(System.Guid)">
          <source>Allows the designer infrastructure to release the actual reference to the activity and avoids memory leak.</source>
          <target state="translated">允許設計工具基礎結構釋放活動的實際參考，並避免記憶體流失。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" uid="E:System.Activities.Presentation.ObjectReferenceService.SourceLocationUpdated">
          <source>Occurs when the source location for the object changes.</source>
          <target state="translated">在物件的來源位置變更時發生。</target>       </trans-unit>
        <trans-unit id="117" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.TryGetObject(System.Guid,System.Object@)">
          <source>The activity reference.</source>
          <target state="translated">活動參考。</target>       </trans-unit>
        <trans-unit id="118" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.TryGetObject(System.Guid,System.Object@)">
          <source>The de-referenced activity, if the reference is available, or null otherwise.</source>
          <target state="translated">如果有參考可用則為已取值的活動，否則為null。</target>       </trans-unit>
        <trans-unit id="119" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.TryGetObject(System.Guid,System.Object@)">
          <source>Obtains the actual reference to the object by its ObjectReference.</source>
          <target state="translated">依其 ObjectReference 取得物件的實際參考。</target>       </trans-unit>
        <trans-unit id="120" translate="yes" xml:space="preserve" uid="M:System.Activities.Presentation.ObjectReferenceService.TryGetObject(System.Guid,System.Object@)">
          <source><ph id="ph1">&lt;see langword="True" /&gt;</ph> if the actual reference to the object is obtained by the object reference; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated">如果物件的實際參考是由物件參考取得，則為 <ph id="ph1">&lt;see langword="True" /&gt;</ph>，否則為 <ph id="ph2">&lt;see langword="false" /&gt;</ph>。</target>       </trans-unit>
