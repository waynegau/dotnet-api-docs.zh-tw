      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Drawing.Text.HotkeyPrefix">
          <source>Specifies the type of display for hot-key prefixes that relate to text.</source>
          <target state="translated">指定關於文字的快速鍵字首之顯示類型。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Drawing.Text.HotkeyPrefix">
          <source>A hot-key prefix allows you to use a keyboard combination (usually CTRL+<bpt id="p1">*</bpt>HotKey<ept id="p1">*</ept> or ALT+<bpt id="p2">*</bpt>HotKey<ept id="p2">*</ept>) to access functionality represented by text displayed on the screen.</source>
          <target state="translated">熱鍵前置詞可讓您使用鍵盤的組合 (通常是 CTRL +<bpt id="p1">*</bpt>熱鍵<ept id="p1">*</ept>或 ALT +<bpt id="p2">*</bpt>熱鍵<ept id="p2">*</ept>) 來存取功能由螢幕上顯示的文字。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Drawing.Text.HotkeyPrefix.Hide">
          <source>Do not display the hot-key prefix.</source>
          <target state="translated">不要顯示快速鍵字首。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Drawing.Text.HotkeyPrefix.None">
          <source>No hot-key prefix.</source>
          <target state="translated">沒有快速鍵字首。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Drawing.Text.HotkeyPrefix.Show">
          <source>Display the hot-key prefix.</source>
          <target state="translated">顯示快速鍵字首。</target>       </trans-unit>
