      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Configuration.MessageSecurityOverTcpElement">
          <source>Defines the configuration settings for the security of a message sent using TCP.</source>
          <target state="translated">為使用 TCP 傳送的訊息定義安全性組態設定。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="T:System.ServiceModel.Configuration.MessageSecurityOverTcpElement">
          <source>This class cannot be inherited.</source>
          <target state="translated">這個類別無法被繼承。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.ServiceModel.Configuration.MessageSecurityOverTcpElement.#ctor">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.MessageSecurityOverTcpElement" /&gt;</ph> class.</source>
          <target state="translated">初始化 <ph id="ph1">&lt;see cref="T:System.ServiceModel.Configuration.MessageSecurityOverTcpElement" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.MessageSecurityOverTcpElement.AlgorithmSuite">
          <source>Gets or sets the message encryption and key-wrap algorithms used to secure a TCP message.</source>
          <target state="translated">取得或設定用於保護 TCP 訊息的訊息加密 (Encryption) 和金鑰包裝演算法。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.MessageSecurityOverTcpElement.AlgorithmSuite">
          <source>A valid <ph id="ph1">&lt;see cref="T:System.ServiceModel.Security.SecurityAlgorithmSuite" /&gt;</ph> value that specifies the message encryption and key-wrap algorithms used to secure a TCP message.</source>
          <target state="translated">有效的 <ph id="ph1">&lt;see cref="T:System.ServiceModel.Security.SecurityAlgorithmSuite" /&gt;</ph> 值，指定用於保護 TCP 訊息的訊息加密和金鑰包裝演算法。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.MessageSecurityOverTcpElement.AlgorithmSuite">
          <source>The default value is <ph id="ph1">&lt;see cref="P:System.ServiceModel.Security.SecurityAlgorithmSuite.Basic256" /&gt;</ph>.</source>
          <target state="translated">預設值是 <ph id="ph1">&lt;see cref="P:System.ServiceModel.Security.SecurityAlgorithmSuite.Basic256" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.MessageSecurityOverTcpElement.ClientCredentialType">
          <source>Specifies the type of credential to be used when performing client authentication using message-based security or <ph id="ph1">&lt;see langword="TransportWithMessageCredential" /&gt;</ph>.</source>
          <target state="translated">指定當使用訊息安全性或 <ph id="ph1">&lt;see langword="TransportWithMessageCredential" /&gt;</ph> 執行用戶端驗證時，要使用的認證類型。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.MessageSecurityOverTcpElement.ClientCredentialType">
          <source>A valid <ph id="ph1">&lt;see cref="T:System.ServiceModel.MessageCredentialType" /&gt;</ph> value that specifies the type of credential to be used when performing client authentication using message-based security or <ph id="ph2">&lt;see langword="TransportWithMessageCredential" /&gt;</ph>.</source>
          <target state="translated">有效的 <ph id="ph1">&lt;see cref="T:System.ServiceModel.MessageCredentialType" /&gt;</ph> 值，指定當使用訊息安全性或 <ph id="ph2">&lt;see langword="TransportWithMessageCredential" /&gt;</ph> 執行用戶端驗證時，要使用的認證類型。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.MessageSecurityOverTcpElement.ClientCredentialType">
          <source>The default value is <ph id="ph1">&lt;see cref="F:System.ServiceModel.MessageCredentialType.Windows" /&gt;</ph>.</source>
          <target state="translated">預設值是 <ph id="ph1">&lt;see cref="F:System.ServiceModel.MessageCredentialType.Windows" /&gt;</ph>。</target>       </trans-unit>
