          <target state="translated">指定資料表名稱允許的字元數目上限。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Xml.XmlDictionaryReaderQuotaTypes.MaxStringContentLength">
