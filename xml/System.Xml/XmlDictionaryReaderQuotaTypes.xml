        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Xml.XmlDictionaryReaderQuotaTypes">
          <source>Enumerates the configurable quota values for XmlDictionaryReaders.</source>
          <target state="translated">列舉 XmlDictionaryReaders 的可設定配額值。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Xml.XmlDictionaryReaderQuotaTypes.MaxArrayLength">
          <source>Specifies the maximum allowed array length.</source>
          <target state="translated">指定允許的陣列長度上限。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Xml.XmlDictionaryReaderQuotaTypes.MaxBytesPerRead">
          <source>Specifies the maximum allowed bytes returned for each read.</source>
          <target state="translated">指定允許每個讀取動作傳回的位元組上限。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Xml.XmlDictionaryReaderQuotaTypes.MaxDepth">
          <source>Specifies the maximum nested node depth.</source>
          <target state="translated">指定巢狀節點深度上限。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Xml.XmlDictionaryReaderQuotaTypes.MaxNameTableCharCount">
