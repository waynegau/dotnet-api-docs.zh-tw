        <trans-unit id="111" translate="yes" xml:space="preserve" uid="M:System.Activities.Statements.TimerExtension.RegisterTimer(System.TimeSpan,System.Activities.Bookmark)">
          <source>The duration of the timer.</source>
          <target state="translated">計時器的持續期間。</target>       </trans-unit>
