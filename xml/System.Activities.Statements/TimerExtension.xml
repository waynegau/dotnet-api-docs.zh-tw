      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.Statements.TimerExtension">
          <source>Abstract base class for timer extensions.</source>
          <target state="translated">計時器擴充的抽象基底類別。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Activities.Statements.TimerExtension">
          <source>A timer extension is used by activities such as <ph id="ph1">&lt;xref:System.Activities.Statements.Delay&gt;</ph> to manage timers.</source>
          <target state="translated">如 <ph id="ph1">&lt;xref:System.Activities.Statements.Delay&gt;</ph> 這類的活動會使用計時器擴充來管理計時器。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Activities.Statements.TimerExtension.#ctor">
          <source>When implemented in a derived class, creates a new instance of the derived class.</source>
          <target state="translated">在衍生類別中實作時，建立衍生類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Activities.Statements.TimerExtension.CancelTimer(System.Activities.Bookmark)">
          <source>The bookmark associated with the timer.</source>
          <target state="translated">與計時器相關聯的書籤。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Activities.Statements.TimerExtension.CancelTimer(System.Activities.Bookmark)">
          <source>Removes a timer.</source>
          <target state="translated">移除計時器。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Activities.Statements.TimerExtension.OnCancelTimer(System.Activities.Bookmark)">
          <source>The bookmark associated with the timer.</source>
          <target state="translated">與計時器相關聯的書籤。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:System.Activities.Statements.TimerExtension.OnCancelTimer(System.Activities.Bookmark)">
          <source>Called by the <ph id="ph1">&lt;see cref="M:System.Activities.Statements.TimerExtension.CancelTimer(System.Activities.Bookmark)" /&gt;</ph> method.</source>
          <target state="translated">由 <ph id="ph1">&lt;see cref="M:System.Activities.Statements.TimerExtension.CancelTimer(System.Activities.Bookmark)" /&gt;</ph> 方法呼叫。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Activities.Statements.TimerExtension.OnRegisterTimer(System.TimeSpan,System.Activities.Bookmark)">
          <source>The duration of the timer.</source>
          <target state="translated">計時器的持續期間。</target>       </trans-unit>
