      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Security.Authentication.HashAlgorithmType">
          <source>Specifies the algorithm used for generating message authentication codes (MACs).</source>
          <target state="translated">指定用來產生訊息驗證碼 (MAC) 的演算法。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Authentication.HashAlgorithmType">
          <source>This enumeration specifies valid values for the <ph id="ph1">&lt;xref:System.Net.Security.SslStream.HashAlgorithm%2A?displayProperty=nameWithType&gt;</ph> property.</source>
          <target state="translated">此列舉會指定有效的值<ph id="ph1">&lt;xref:System.Net.Security.SslStream.HashAlgorithm%2A?displayProperty=nameWithType&gt;</ph>屬性。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.Authentication.HashAlgorithmType">
          <source>The following example displays the properties of an <ph id="ph1">&lt;xref:System.Net.Security.SslStream&gt;</ph> after authentication has succeeded.</source>
          <target state="translated">下列範例顯示的屬性<ph id="ph1">&lt;xref:System.Net.Security.SslStream&gt;</ph>成功驗證之後。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.HashAlgorithmType.Md5">
          <source>The Message Digest 5 (MD5) hashing algorithm.</source>
          <target state="translated">訊息摘要 5 (MD5) 雜湊演算法。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.HashAlgorithmType.None">
          <source>No hashing algorithm is used.</source>
          <target state="translated">未使用任何雜湊演算法。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.HashAlgorithmType.Sha1">
          <source>The Secure Hashing Algorithm (SHA1).</source>
          <target state="translated">安全雜湊演算法 (SHA1)。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.HashAlgorithmType.Sha256">
          <source>The Secure Hashing Algorithm 2 (SHA-2), using a 256-bit digest.</source>
          <target state="translated">使用 256 位元摘要的安全雜湊演算法 2 (SHA-2)。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.HashAlgorithmType.Sha384">
          <source>The Secure Hashing Algorithm 2 (SHA-2), using a 384-bit digest.</source>
          <target state="translated">使用 384 位元摘要的安全雜湊演算法 2 (SHA-2)。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Security.Authentication.HashAlgorithmType.Sha512">
          <source>The Secure Hashing Algorithm 2 (SHA-2), using a 512-bit digest.</source>
          <target state="translated">使用 512 位元摘要的安全雜湊演算法 2 (SHA-2)。</target>       </trans-unit>
