      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Controls.DataGridGridLinesVisibility">
          <source>Defines constants that specify the visibility of the grid lines in a <ph id="ph1">&lt;see cref="T:System.Windows.Controls.DataGrid" /&gt;</ph>.</source>
          <target state="translated">定義常數，以指定 <ph id="ph1">&lt;see cref="T:System.Windows.Controls.DataGrid" /&gt;</ph> 中的格線可見度。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Controls.DataGridGridLinesVisibility">
          <source>This enumeration is used by the <ph id="ph1">&lt;xref:System.Windows.Controls.DataGrid.GridLinesVisibility%2A&gt;</ph> property.</source>
          <target state="translated">這個列舉型別由<ph id="ph1">&lt;xref:System.Windows.Controls.DataGrid.GridLinesVisibility%2A&gt;</ph>屬性。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Windows.Controls.DataGridGridLinesVisibility">
          <source>The style of the grid lines is determined by the <ph id="ph1">&lt;xref:System.Windows.Controls.DataGrid.VerticalGridLinesBrush%2A&gt;</ph> and <ph id="ph2">&lt;xref:System.Windows.Controls.DataGrid.HorizontalGridLinesBrush%2A&gt;</ph> properties.</source>
          <target state="translated">格線的樣式由<ph id="ph1">&lt;xref:System.Windows.Controls.DataGrid.VerticalGridLinesBrush%2A&gt;</ph>和<ph id="ph2">&lt;xref:System.Windows.Controls.DataGrid.HorizontalGridLinesBrush%2A&gt;</ph>屬性。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridGridLinesVisibility.All">
          <source>Both horizontal and vertical grid lines are visible.</source>
          <target state="translated">水平和垂直格線皆為可見的。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridGridLinesVisibility.Horizontal">
          <source>Only horizontal grid lines are visible.</source>
          <target state="translated">只有水平格線為可見的。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridGridLinesVisibility.None">
          <source>No grid lines are visible.</source>
          <target state="translated">沒有任何格線為可見的。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Windows.Controls.DataGridGridLinesVisibility.Vertical">
          <source>Only vertical grid lines are visible.</source>
          <target state="translated">只有垂直格線為可見的。</target>       </trans-unit>
