      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.Win32.UserPreferenceChangedEventArgs">
          <source>Provides data for the <ph id="ph1">&lt;see cref="E:Microsoft.Win32.SystemEvents.UserPreferenceChanged" /&gt;</ph> event.</source>
          <target state="translated">提供 <ph id="ph1">&lt;see cref="E:Microsoft.Win32.SystemEvents.UserPreferenceChanged" /&gt;</ph> 事件的資料。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.Win32.UserPreferenceChangedEventArgs">
          <source>A <ph id="ph1">&lt;xref:Microsoft.Win32.SystemEvents.UserPreferenceChanged&gt;</ph> event occurs when a user preference has changed.</source>
          <target state="translated">A<ph id="ph1">&lt;xref:Microsoft.Win32.SystemEvents.UserPreferenceChanged&gt;</ph>使用者喜好設定變更時，就會發生事件。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:Microsoft.Win32.UserPreferenceChangedEventArgs.#ctor(Microsoft.Win32.UserPreferenceCategory)">
          <source>One of the <ph id="ph1">&lt;see cref="T:Microsoft.Win32.UserPreferenceCategory" /&gt;</ph> values that indicates the user preference category that has changed.</source>
          <target state="translated">其中一個 <ph id="ph1">&lt;see cref="T:Microsoft.Win32.UserPreferenceCategory" /&gt;</ph> 值，表示已經變更的使用者喜好設定分類。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:Microsoft.Win32.UserPreferenceChangedEventArgs.#ctor(Microsoft.Win32.UserPreferenceCategory)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:Microsoft.Win32.UserPreferenceChangedEventArgs" /&gt;</ph> class using the specified user preference category identifier.</source>
          <target state="translated">使用指定的使用者喜好設定分類識別項來初始化 <ph id="ph1">&lt;see cref="T:Microsoft.Win32.UserPreferenceChangedEventArgs" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:Microsoft.Win32.UserPreferenceChangedEventArgs.Category">
          <source>Gets the category of user preferences that has changed.</source>
          <target state="translated">取得已經變更的使用者喜好設定分類。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Win32.UserPreferenceChangedEventArgs.Category">
          <source>One of the <ph id="ph1">&lt;see cref="T:Microsoft.Win32.UserPreferenceCategory" /&gt;</ph> values that indicates the category of user preferences that has changed.</source>
