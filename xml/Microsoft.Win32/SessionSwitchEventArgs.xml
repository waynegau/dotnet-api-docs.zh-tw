        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.Win32.SessionSwitchEventArgs">
          <source>Provides data for the <ph id="ph1">&lt;see cref="E:Microsoft.Win32.SystemEvents.SessionSwitch" /&gt;</ph> event.</source>
          <target state="translated">提供 <ph id="ph1">&lt;see cref="E:Microsoft.Win32.SystemEvents.SessionSwitch" /&gt;</ph> 事件的資料。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.Win32.SessionSwitchEventArgs">
          <source>A <ph id="ph1">&lt;xref:Microsoft.Win32.SystemEvents.SessionSwitch&gt;</ph> event is raised when the current user changes.</source>
          <target state="translated">A<ph id="ph1">&lt;xref:Microsoft.Win32.SystemEvents.SessionSwitch&gt;</ph>目前的使用者變更時，會引發事件。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:Microsoft.Win32.SessionSwitchEventArgs.#ctor(Microsoft.Win32.SessionSwitchReason)">
          <source>A <ph id="ph1">&lt;see cref="T:Microsoft.Win32.SessionSwitchReason" /&gt;</ph> that indicates the type of session change event.</source>
          <target state="translated"><ph id="ph1">&lt;see cref="T:Microsoft.Win32.SessionSwitchReason" /&gt;</ph> ，表示工作階段變更事件的型別。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:Microsoft.Win32.SessionSwitchEventArgs.#ctor(Microsoft.Win32.SessionSwitchReason)">
