      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Messaging.MessageLookupAction">
          <source>Specifies a message to peek at or receive from a message queue.</source>
          <target state="translated">指定要從訊息佇列窺視或接收的訊息。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Messaging.MessageLookupAction">
          <source>The <ph id="ph1">&lt;xref:System.Messaging.MessageLookupAction&gt;</ph> enumeration enables you to specify a message to peek at or receive from a message queue.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Messaging.MessageLookupAction&gt;</ph>列舉型別可讓您指定要查看或接收訊息佇列的訊息。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Messaging.MessageLookupAction.Current">
          <source>Read the current message.</source>
          <target state="translated">讀取目前的訊息。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Messaging.MessageLookupAction.First">
          <source>Read the first message in the queue.</source>
          <target state="translated">讀取佇列中的第一則訊息。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Messaging.MessageLookupAction.Last">
          <source>Read the last message in the queue.</source>
          <target state="translated">讀取佇列中的最後一則訊息。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Messaging.MessageLookupAction.Next">
          <source>Read the following message.</source>
          <target state="translated">讀取下列訊息。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Messaging.MessageLookupAction.Previous">
          <source>Read the preceding message.</source>
          <target state="translated">讀取前一則訊息。</target>       </trans-unit>
