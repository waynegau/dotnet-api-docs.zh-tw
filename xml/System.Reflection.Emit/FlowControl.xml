        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Reflection.Emit.FlowControl">
          <source>Describes how an instruction alters the flow of control.</source>
          <target state="translated">描述指令如何變更控制流程。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Reflection.Emit.FlowControl.Branch">
