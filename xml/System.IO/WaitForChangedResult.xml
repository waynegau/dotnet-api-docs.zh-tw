      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.IO.WaitForChangedResult">
          <source>Contains information on the change that occurred.</source>
          <target state="translated">包含有關發生的變更之資訊。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.IO.WaitForChangedResult">
          <source>This example shows how to use a <ph id="ph1">&lt;xref:System.IO.FileSystemWatcher&gt;</ph> synchronously by having it wait for a specific file change notification to occur.</source>
          <target state="translated">這個範例示範如何使用<ph id="ph1">&lt;xref:System.IO.FileSystemWatcher&gt;</ph>同步的等候特定的檔案變更通知，就會發生。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="P:System.IO.WaitForChangedResult.ChangeType">
          <source>Gets or sets the type of change that occurred.</source>
          <target state="translated">取得或設定發生的變更類型。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.IO.WaitForChangedResult.ChangeType">
          <source>One of the <ph id="ph1">&lt;see cref="T:System.IO.WatcherChangeTypes" /&gt;</ph> values.</source>
          <target state="translated">其中一個 <ph id="ph1">&lt;see cref="T:System.IO.WatcherChangeTypes" /&gt;</ph> 值。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="P:System.IO.WaitForChangedResult.Name">
          <source>Gets or sets the name of the file or directory that changed.</source>
          <target state="translated">取得或設定變更的檔案或目錄的名稱。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.IO.WaitForChangedResult.Name">
          <source>The name of the file or directory that changed.</source>
          <target state="translated">變更的檔案或目錄的名稱。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.IO.WaitForChangedResult.Name">
          <source>The following example demonstrates the <ph id="ph1">&lt;xref:System.IO.WaitForChangedResult.Name%2A&gt;</ph> property.</source>
          <target state="translated">下列範例會示範<ph id="ph1">&lt;xref:System.IO.WaitForChangedResult.Name%2A&gt;</ph>屬性。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.IO.WaitForChangedResult.Name">
