      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Activities.Tracking.ActivityStateRecord">
          <source>Represents a tracking record that is created when an activity changes state.</source>
          <target state="translated">代表在活動變更狀態時建立的追蹤記錄。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityStateRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.String)">
          <source>The instance ID of the activity.</source>
          <target state="translated">活動的執行個體 ID。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityStateRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.String)">
          <source>A sequence that defines the order in which tracking records are generated.</source>
          <target state="translated">定義產生追蹤記錄順序的序列。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityStateRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.String)">
          <source>The activity.</source>
          <target state="translated">活動。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityStateRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.String)">
          <source>The activity state.</source>
          <target state="translated">活動狀態。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityStateRecord.#ctor(System.Guid,System.Int64,System.Activities.Tracking.ActivityInfo,System.String)">
          <source>Initializes a new instance of the <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityStateRecord" /&gt;</ph> class using the specified instance ID, record number, activity and activity state.</source>
          <target state="translated">使用指定執行個體 ID、記錄編號、活動與活動狀態，初始化 <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityStateRecord" /&gt;</ph> 類別的新執行個體。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="P:System.Activities.Tracking.ActivityStateRecord.Activity">
          <source>Gets an <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph> that contains information on the activity when the record is generated.</source>
          <target state="translated">取得 <ph id="ph1">&lt;see cref="T:System.Activities.Tracking.ActivityInfo" /&gt;</ph>，其中包含產生記錄時活動的相關資訊。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Tracking.ActivityStateRecord.Activity">
          <source>The activity information.</source>
          <target state="translated">活動資訊。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="P:System.Activities.Tracking.ActivityStateRecord.Arguments">
          <source>Gets the current values of the tracked arguments associated with the activity when the record is generated.</source>
          <target state="translated">產生記錄時，取得與活動相關聯之已追蹤引數目前的值。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Tracking.ActivityStateRecord.Arguments">
          <source>A <ph id="ph1">&lt;see cref="T:System.Collections.Generic.IDictionary`2" /&gt;</ph> containing the arguments.</source>
          <target state="translated">包含引數的 <ph id="ph1">&lt;see cref="T:System.Collections.Generic.IDictionary`2" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="P:System.Activities.Tracking.ActivityStateRecord.State">
          <source>Gets the current state of the activity when the record is generated.</source>
          <target state="translated">取得產生記錄時活動的目前狀態。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Tracking.ActivityStateRecord.State">
          <source>The activity state.</source>
          <target state="translated">活動狀態。</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityStateRecord.ToString">
          <source>Returns a string that contains all the details of the tracking record.</source>
          <target state="translated">傳回字串，其中包含追蹤記錄的所有詳細資料。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:System.Activities.Tracking.ActivityStateRecord.ToString">
          <source>The tracking data.</source>
          <target state="translated">追蹤資料。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="P:System.Activities.Tracking.ActivityStateRecord.Variables">
          <source>Gets the current values of the tracked variables associated with the activity when the record is generated.</source>
          <target state="translated">產生記錄時，取得與活動相關聯之已追蹤變數目前的值。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Activities.Tracking.ActivityStateRecord.Variables">
          <source>The current values.</source>
          <target state="translated">目前的值。</target>       </trans-unit>
