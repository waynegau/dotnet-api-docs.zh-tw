      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Security.RightsManagement.AuthenticationType">
          <source>Specifies the method of rights management authentication.</source>
          <target state="translated">指定權限管理驗證的方法。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.RightsManagement.AuthenticationType">
          <source><ph id="ph1">&lt;xref:System.Security.RightsManagement.AuthenticationType&gt;</ph> specifies how users who access rights managed content are to be authenticated.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Security.RightsManagement.AuthenticationType&gt;</ph> 指定要驗證的使用者存取權限管理的內容的方式。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.RightsManagement.AuthenticationType">
          <source><ph id="ph1">`Internal`</ph> also provides authentication for the owner and author of a protected document or template.</source>
          <target state="translated"><ph id="ph1">`Internal`</ph> 也會提供驗證的擁有者和受保護的文件或範本的作者。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Security.RightsManagement.AuthenticationType">
          <source>The following example also shows use of the <ph id="ph1">&lt;xref:System.Security.RightsManagement.AuthenticationType&gt;</ph> enumeration.</source>
          <target state="translated">下列範例也會顯示使用<ph id="ph1">&lt;xref:System.Security.RightsManagement.AuthenticationType&gt;</ph>列舉型別。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Security.RightsManagement.AuthenticationType.Internal">
          <source>Implicit authentication to any requesting user.</source>
          <target state="translated">任何要求使用者的隱含驗證。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Security.RightsManagement.AuthenticationType.Passport">
          <source><ph id="ph1">[!INCLUDE[TLA#tla_passport](~/includes/tlasharptla-passport-md.md)]</ph> authentication.</source>
          <target state="translated"><ph id="ph1">[!INCLUDE[TLA#tla_passport](~/includes/tlasharptla-passport-md.md)]</ph> 驗證。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Security.RightsManagement.AuthenticationType.Windows">
          <source><ph id="ph1">[!INCLUDE[TLA#tla_mswin](~/includes/tlasharptla-mswin-md.md)]</ph> authentication in a corporate domain environment.</source>
          <target state="translated">企業網域環境中的 <ph id="ph1">[!INCLUDE[TLA#tla_mswin](~/includes/tlasharptla-mswin-md.md)]</ph> 驗證。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Security.RightsManagement.AuthenticationType.WindowsPassport">
          <source>Either Windows authentication or <ph id="ph1">[!INCLUDE[TLA2#tla_passport](~/includes/tla2sharptla-passport-md.md)]</ph> authentication.</source>
          <target state="translated">Windows 驗證或 <ph id="ph1">[!INCLUDE[TLA2#tla_passport](~/includes/tla2sharptla-passport-md.md)]</ph> 驗證。</target>       </trans-unit>
