        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Xaml.Schema.ShouldSerializeResult">
          <source>Defines serialization behavior as reported by a <ph id="ph1">&lt;see cref="T:System.Xaml.Schema.XamlMemberInvoker" /&gt;</ph>.</source>
          <target state="translated">定義 <ph id="ph1">&lt;see cref="T:System.Xaml.Schema.XamlMemberInvoker" /&gt;</ph> 會報告的序列化行為。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Xaml.Schema.ShouldSerializeResult">
          <source>This is for specialized serialization support by invokers.</source>
          <target state="translated">這是特殊的序列化支援，由啟動程式。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Xaml.Schema.ShouldSerializeResult">
          <source>More general serialization support should not require this enumeration.</source>
          <target state="translated">更一般的序列化支援應該不需要這個列舉型別。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Xaml.Schema.ShouldSerializeResult">
          <source><ph id="ph1">`ShouldSerialize`</ph> information is otherwise available to XAML writers as part of the default XAML schema context and CLR reflection.</source>
          <target state="translated"><ph id="ph1">`ShouldSerialize`</ph> 否則使用預設 XAML 結構描述內容及 CLR 反映一部分的 XAML 寫入器資訊。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Xaml.Schema.ShouldSerializeResult.Default">
          <source>Unknown, defer to the type of the member.</source>
          <target state="translated">未知，由成員的型別決定。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Xaml.Schema.ShouldSerializeResult.False">
          <source>Do not serialize the result.</source>
          <target state="translated">不要序列化結果。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Xaml.Schema.ShouldSerializeResult.True">
          <source>Serialize the result.</source>
          <target state="translated">序列化結果。</target>       </trans-unit>
